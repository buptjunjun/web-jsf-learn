<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE hibernate-mapping PUBLIC  
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
<hibernate-mapping package="org.easyGoingCrawler.hibernate">  
    <class name="User" table="usermodel">  
            <id name="uid"></id>  
            <property name="age" />     
            <property name="lastCrawlTime"/>        
    </class>    
</hibernate-mapping>  
