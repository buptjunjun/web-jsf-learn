<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE hibernate-mapping PUBLIC  
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
<hibernate-mapping package="org.easyGoingCrawler.framwork">  
    <class name="CrawlURI" table="urlstore">  
            <id name="url"></id>  
            <property name="host" />    
            <property name="status" />     
            <property name="httpstatus"/>  
            <property name="tags"/>    
            <property name="encode"/>    
            <property name="path"/>    
            <property name="collectDate"/>  
            <property name="lastCrawlDate"/>           
    </class>
</hibernate-mapping>      